<owl:NamedIndividual rdf:about="#HV01">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành biển báo, vạch kẻ đường</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành hiệu lệnh, chỉ dẫn của biển báo hiệu hoặc vạch kẻ đường khi tham gia giao thông,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV02">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành đèn tín hiệu</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành hiệu lệnh của đèn tín hiệu giao thông (vượt đèn đỏ hoặc đèn vàng),,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV03">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành hiệu lệnh chỉ huy</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không chấp hành hiệu lệnh, hướng dẫn của người điều khiển giao thông hoặc người kiểm soát giao thông,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV04">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chuyển hướng trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chuyển hướng không giảm tốc độ, không có tín hiệu báo hướng rẽ hoặc không nhường đường cho đối tượng ưu tiên,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV05">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dừng xe trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dừng xe không sát lề đường, dừng tại nơi có biển cấm, trên cầu, trong hầm hoặc tại các vị trí gây cản trở,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV06">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đỗ xe trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đỗ xe tại nơi có biển cấm, đỗ trên hè phố trái phép, không sát lề đường hoặc không đặt biển báo nguy hiểm khi cần thiết,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV07">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chuyển làn trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chuyển làn đường không đúng nơi cho phép hoặc không có tín hiệu báo trước,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV08">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bấm còi, sử dụng đèn trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bấm còi, rú ga liên tục; dùng đèn chiếu xa trong đô thị hoặc khi tránh xe ngược chiều; bấm còi đêm,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV09">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chạy quá tốc độ</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Điều khiển phương tiện chạy vượt quá tốc độ tối đa cho phép trên đoạn đường đang lưu thông,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV10">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm khoảng cách an toàn</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không giữ khoảng cách an toàn để xảy ra va chạm hoặc không tuân thủ biển báo cự ly tối thiểu,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV11">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sử dụng thiết bị khi lái xe</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dùng tay sử dụng điện thoại di động, ô (dù), thiết bị âm thanh khi đang điều khiển phương tiện,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV12">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đi vào đường cấm</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đi vào khu vực cấm, đường có biển báo hiệu có nội dung cấm phương tiện đi vào,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV13">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đi ngược chiều</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đi ngược chiều của đường một chiều hoặc đường có biển cấm đi ngược chiều,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV14">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vượt xe trái quy định</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vượt xe tại nơi cấm vượt, vượt bên phải trái phép hoặc vượt không có báo hiệu,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV15">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Đi sai phần đường, làn đường</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không đi bên phải theo chiều đi, đi sai làn đường quy định hoặc đi trên hè phố,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV16">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm nồng độ cồn</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Điều khiển phương tiện mà trong máu hoặc khí thở có nồng độ cồn,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV17">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sử dụng ma túy</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Điều khiển phương tiện trên đường mà trong cơ thể có chất ma túy,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV18">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm quy định mũ bảo hiểm</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không đội mũ bảo hiểm hoặc đội mũ nhưng cài quai không đúng quy cách cho người lái hoặc người chở,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV19">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm quy định dây an toàn</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không thắt dây an toàn khi xe đang chạy tại vị trí có trang bị dây an toàn,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV20">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lạng lách, đánh võng</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Điều khiển phương tiện lạng lách, đánh võng, chạy quá tốc độ đuổi nhau hoặc đi xe một bánh,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV21">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm trách nhiệm khi xảy ra tai nạn</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không dừng lại, không giữ hiện trường, bỏ trốn hoặc không cứu giúp người bị nạn,,,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV22">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vi phạm quy định về giấy tờ</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Không có, không mang theo hoặc sử dụng giấy tờ (GPLX, Đăng ký, Kiểm định, Bảo hiểm) giả, tẩy xóa,,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV23">
  <rdf:type rdf:resource="#HanhVi"/>
  <ten_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gây nguy hiểm bằng vật thể</ten_hanh_vi>
  <mo_ta_hanh_vi rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rải đinh, vật sắc nhọn, chăng dây hoặc ném vật thể vào phương tiện đang di chuyển,.</mo_ta_hanh_vi>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#HV24">
